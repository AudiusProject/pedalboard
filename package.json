{
  "name": "pedalboard",
  "version": "0.0.30",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "type-check": "turbo run lint --filter='[HEAD^1]'",
    "docker": "./scripts/docker.sh",
    "docker:verified-notifications": "./scripts/docker.sh verified-notifications",
    "docker:crm": "./scripts/docker.sh crm",
    "docker:archiver": "./scripts/docker.sh archiver",
    "docker:staking": "./scripts/docker.sh staking",
    "docker:trending-challenge-rewards": "./scripts/docker.sh trending-challenge-rewards",
    "docker:mri": "./scripts/docker.sh mri",
    "docker:relay": "./scripts/docker.sh relay",
    "docker:solana-relay": "./scripts/docker.sh solana-relay",
    "docker:app-template": "./scripts/docker.sh app-template",
    "docker:anti-abuse-oracle": "./scripts/docker.sh anti-abuse-oracle"
  },
  "devDependencies": {
    "prettier": "2.8.8",
    "turbo": "1.10.16",
    "typescript": "4.5.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "packageManager": "npm@8.19.4"
}