  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.17",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "concurrently": "^8.2.2",
