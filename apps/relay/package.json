    "@connectrpc/connect-node": "2.0.1",
    "@ethersproject/abstract-provider": "^5.8.0",
    "@grpc/grpc-js": "^1.11.3",
    "@grpc/proto-loader": "^0.7.13",
    "@pedalboard/basekit": "*",
    "@pedalboard/logger": "*",
    "@pedalboard/storage": "*",
    "@types/compute-lcm": "^1.1.0",
    "axios": "^0.19.2",
    "body-parser": "^1.20.2",
    "compute-lcm": "^1.1.2",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "envalid": "^7.3.1",
    "eth-sig-util": "^3.0.1",
    "ethers": "^5.7.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
