{
  "name": "@pedalboard/archiver",
  "version": "0.0.1",
  "description": "Creates archive files of tracks for a given entity",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "start": "node ./dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@audius/sdk": "*",
    "@pedalboard/basekit": "*",
    "@pedalboard/logger": "*",
    "@pedalboard/storage": "*",
    "archiver": "7.0.1",
    "bullmq": "5.41.7",
    "cors": "2.8.5",
    "envalid": "8.0.0",
    "express": "4.17.1",
    "node-fetch": "2.7.0",
    "pino-http": "10.4.0"
  },
  "devDependencies": {
    "@types/archiver": "6.0.3",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.12",
    "@types/node": "18.19.79",
    "esbuild": "0.14.38",
    "esbuild-register": "3.3.2",
    "eslint": "8.56.0",
    "eslint-config-custom-server": "*",
    "ts-node": "10.9.2",
    "tsconfig": "*",
    "typescript": "5.0.4",
    "vitest": "0.34.6"
  }
}
